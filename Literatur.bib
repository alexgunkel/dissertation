@Article{Wurtz:TschirnhausunddieSpinozismusbeschuldigung1981,
  Title                    = {Tschirnhaus und die Spinozismusbeschuldigung},
  Author                   = {Wurtz, Jean-Paul},
  Journal                  = {Studia Leibnitiana},
  Number                   = {1},
  Pages                    = {61--75},
  Subtitle                 = {Die Polemik mit Christian Thomasius},
  Volume                   = {13},
  Year                     = {1981},

  Abstract                 = {La publication de la Medicina mentis par Tschirnhaus devait donner lieu à une vive polémique avec C. Thomasius (critique de l'œuvre par Thomasius, riposte de Tschirnhaus et réponse de Thomasius, enfin projet d'une réplique de Tschirnhaus à ces nouvelles attaques). Violemment hostile à la pensée de Spinoza, Thomasius s'attacha entre autres à dénoncer l'influence de ce philosophe sur Tschirnhaus concernant la conception de la méthode, l'éthique et la doctrine de Dieu. De crainte de passer pour un disciple de cet auteur, Tschirnhaus essaya désespérément non seulement de signaler ce qui le sépare de lui, mais encore de nier, contre toute évidence, toute influence de sa part. On en retiendra essentiellement que, malgré ses formules d'inspiration spinoziste, Tschirnhaus ne semble pas avoir admis les thèses centrales de la métaphysique de Spinoza, ce qui a entraîné des divergences jusque sur le plan de l'éthique, mais aussi qu'en depit de cela, il a pu propager au moins celles des conceptions de Spinoza qu'il approuvait malgré ses délégations.},
  Copyright                = {Copyright © 1981 Franz Steiner Verlag},
