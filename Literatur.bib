@Article{Finster:ZurKritikvonChristianAugustCrusiusanderTheoriedereinfachenSubstanzenbeiLeibnizundWolff1986,
  Title                    = {Zur Kritik von Christian August Crusius an der Theorie der einfachen Substanzen bei Leibniz und Wolff},
  Author                   = {Finster, Reinhard},
  Journal                  = {Studia Leibnitiana},
  Number                   = {1},
  Pages                    = {72--82},
  Volume                   = {18},
  Year                     = {1986},

  Abstract                 = {Crusius est consideré comme l'un des plus importants critiques des théories philosophiques de Leibniz et de Wolff. Dans ce contexte, outre la critique de l'interprétation du principe de la raison telle que l'ont soutenue Leibniz et Wolff, il faut surtout citer le refus de l'utilisation de la méthode mathématique dans la philosophie. Le développement de la théorie des substances simples conçue par Crusius servira d'exemple pour démontrer le refus de cette méthode dans la métaphysique. La définition de la substance simple, à laquelle Crusius parvient en ce fondant sur la distinction entre raison d'être et raison de connaître, s'offre à la pensée de Leibniz comme à la pensée de Wolff. Cependant, quand Crusius rejette l'interprétation de la substance simple comme point mathématique, il attaque surtout Leibniz. Ainsi la théorie de Crusius des substances simples doit être avant tout considéré comme une critique de la monadologie leibnizienne.},
  Copyright                = {Copyright © 1986 Franz Steiner Verlag},
  Jstor_articletype        = {research-article},
  Jstor_formatteddate      = {1986},
  Keywords                 = {Enlightenment, Christian August Crusius, Christian Wolff, Leibniz},
  Language                 = {English},
  Publisher                = {Franz Steiner Verlag},
  Timestamp                = {2013.04.26},
  Url                      = {http://www.jstor.org/stable/40694041}
}

@Article{Finster:SpontaneitaetFreiheitundunbedingteKausalitaetbeiLeibnizCrusiusundKant1982,
  Title                    = {Spontaneität, Freiheit und unbedingte Kausalität bei Leibniz, Crusius und Kant},
  Author                   = {Finster, Reinhard},
  Journal                  = {Studia Leibnitiana},
  Number                   = {2},
  Pages                    = {266--277},
  Volume                   = {14},
  Year                     = {1982},

  Abstract                 = {In the Nova dilucidano Kant defines freedom as libertas spontaneitatis. Free acting is always determined acting. It differs from necessary acting by the fact that the determinating reasons are inherent in the acting subject. These reasons must not only be spontaneous but also rational ones. By this, Kant agrees in the main with the Leibnizian and Wolf fian conception of freedom. At the same time Kant refuses expressly the conception of freedom of Crusius. According to Crusius, free acting can not be determined acting, because freedom is defined as unconditioned causality. This conception of freedom Kant postulates in the Critic of pure reason. The proof of the possibility of freedom given by Kant there differs however entirely from the proofs of freedom given by Crusius. According to Kant, it is not impossible to think freedom, because one and the same acting, one and the same phenomenon can be thought at the same time caused by laws of nature and by unconditioned causality, that is freedom, without any contradiction. This central argument of Kant's has an equivalent in the late Leibnizian theory of substance — not in the theory of freedom. Unconditioned causality as the causality of the vis activa primitiva and laws of nature as conditioned causality do not exclude each other at the same time in one and the same acting or phenomenon. Consequently already Leibniz postulates the Kantian thesis that the relation between unconditioned and conditioned causality is not a really disjunctive one.},
  Copyright                = {Copyright © 1982 Franz Steiner Verlag},
  Jstor_articletype        = {research-article},
  Jstor_formatteddate      = {1982},
  Keywords                 = {Enlightenment, Leibniz, Christian August Crusius, Immanuel Kant, Historisches},
  Language                 = {English},
  Publisher                = {Franz Steiner Verlag},
  Timestamp                = {2013.04.26},
  Url                      = {http://www.jstor.org/stable/40693949}
}

